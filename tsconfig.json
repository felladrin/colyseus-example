{
  "compilerOptions": {
    "target": "ES2015",
    "moduleResolution": "Node",
    "outDir": "dist",
    "strict": true,
    "allowJs": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "preserveSymlinks": true
  }
}
